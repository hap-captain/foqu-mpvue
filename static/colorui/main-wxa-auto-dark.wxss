    image.loading::before {
        background-color: rgb(24, 24, 24);
    }

    image.loading::after {
        color: rgb(55, 55, 55);
    }

    radio.radio[checked]::after {
        background-color: rgba(0, 0, 0, 0);
    }

    switch.gray[checked] .wx-switch-input,
    checkbox.gray[checked] .wx-checkbox-input,
    radio.gray[checked] .wx-radio-input {
        background-color: rgb(28, 28, 28) !important;
    }

    .solid::after, .solid-top::after, .solid-right::after, .solid-bottom::after,
    .solid-left::after, .solids::after, .solids-top::after, .solids-right::after,
    .solids-bottom::after, .solids-left::after, .dashed::after,
    .dashed-top::after, .dashed-right::after, .dashed-bottom::after,
    .dashed-left::after {
        border-radius: rgb(0, 0, 0);
    }

    .solids::after {
        border: rgb(0, 0, 0);
    }

    .solids-top::after {
        border-top: rgb(0, 0, 0);
    }

    .solids-right::after {
        border-right: rgb(0, 0, 0);
    }

    .solids-bottom::after {
        border-bottom: rgb(0, 0, 0);
    }

    .solids-left::after {
        border-left: rgb(0, 0, 0);
    }

    .dashed::after {
        border: rgb(0, 0, 0);
    }

    .dashed-top::after {
        border-top: rgb(0, 0, 0);
    }

    .dashed-right::after {
        border-right: rgb(0, 0, 0);
    }

    .dashed-bottom::after {
        border-bottom: rgb(0, 0, 0);
    }

    .dashed-left::after {
        border-left: rgb(0, 0, 0);
    }

    .shadow-blur::before {
        background: rgb(0, 0, 0);
        border-radius: rgb(0, 0, 0);
    }

    .cu-btn:not([class*="bg-"]) {
        background-color: rgb(28, 28, 28);
    }

    .cu-btn[class*="line"] {
        background-color: rgba(0, 0, 0, 0);
    }

    .cu-tag[class*="line-"]::after {
        border-radius: rgb(0, 0, 0);
    }

    .cu-tag.badge:not([class*="bg-"]) {
        background-color: rgb(209, 70, 67);
    }

    .cu-avatar {
        background-color: rgb(55, 55, 55);
    }

    .cu-progress {
        background-color: rgb(27, 30, 35);
    }

    @keyframes progress-stripes {
        from {
            background-position: 72rpx 0;
        }

        to {
            background-position: 0 0;
        }
    }

    .load-progress .load-progress-spinner::after {
        border-top-color: rgb(0, 0, 0);
        border-left-color: rgb(0, 0, 0);
    }

    @keyframes load-progress-spinner {
        0% {
            -webkit-transform: rotate(0);
            transform: rotate(0);
        }

        100% {
            -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
        }
    }

    .cu-list.menu>.cu-item:after {
        border-bottom: rgb(0, 0, 0);
        border-radius: rgb(0, 0, 0);
    }

    .cu-list.menu>.cu-item.grayscale {
        background-color: rgb(24, 24, 24);
    }

    .cu-list.menu>.cu-item.cur {
        background-color: rgb(26, 23, 10);
    }

    .cu-list.menu>.cu-item button.content {
        background-color: rgba(0, 0, 0, 0);
    }

    .cu-list.grid>.cu-item:after {
        border-radius: rgb(0, 0, 0);
    }

    .cu-list.grid>.cu-item text {
        color: rgb(115, 115, 115);
    }

    .cu-bar .search-form {
        background-color: rgb(24, 24, 24);
    }

    .cu-bar .search-form input {
